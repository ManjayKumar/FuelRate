<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Tblactivities" table="tblactivities">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="activityname" type="text" column="activityname" length="65535" nullable="false"/>
    <field name="description" type="text" column="description" length="65535" nullable="false"/>
    <field name="activityDate" type="date" column="activity_date" nullable="false"/>
  </entity>
</doctrine-mapping>
