<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Tblfaculty" table="tblfaculty">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="facultyid" type="integer" column="facultyid" nullable="false"/>
    <field name="lname" type="string" column="lname" length="20" nullable="false"/>
    <field name="fname" type="string" column="fname" length="20" nullable="false"/>
    <field name="mname" type="string" column="mname" length="20" nullable="false"/>
    <field name="address" type="text" column="address" length="65535" nullable="false"/>
    <field name="contact" type="integer" column="contact" nullable="false"/>
    <field name="username" type="string" column="username" length="50" nullable="false"/>
    <field name="password" type="string" column="password" length="50" nullable="false"/>
  </entity>
</doctrine-mapping>
