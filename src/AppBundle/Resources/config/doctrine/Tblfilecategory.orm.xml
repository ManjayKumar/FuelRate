<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Tblfilecategory" table="tblfilecategory">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="categoryname" type="string" column="categoryname" length="50" nullable="false"/>
    <field name="description" type="text" column="description" length="65535" nullable="false"/>
    <field name="deadline" type="string" column="deadline" length="20" nullable="false"/>
  </entity>
</doctrine-mapping>
