<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Tblinbox" table="tblinbox">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="messageDate" type="date" column="message_date" nullable="false"/>
    <field name="subject" type="text" column="subject" length="65535" nullable="false"/>
    <field name="message" type="text" column="message" length="65535" nullable="false"/>
    <field name="sendto" type="integer" column="sendto" nullable="false"/>
    <field name="senderid" type="integer" column="senderid" nullable="false"/>
    <field name="senderName" type="string" column="sender_name" length="20" nullable="false"/>
  </entity>
</doctrine-mapping>
