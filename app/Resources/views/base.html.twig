<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Tyon Technology{% endblock %}</title>
        {% block stylesheets %}
            <link  href="{{ asset('bundle/app/css/semantic.min.css') }} " rel="stylesheet">
            <link  href="{{ asset('css/style.css') }} " rel="stylesheet">
            <link  href="{{ asset('css/salespages.css') }} " rel="stylesheet">
            <link  href="{{ asset('css/icon.min.css') }} " rel="stylesheet">
            
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        
    </head>
    <body>
        {% block body %}

           
    <header>
        <div class="d-header">
            <div class="ui container">
                <div class="ui grid margin0">
                    <div class="sixteen wide column">
                        <div class="logo">
                            <a href="https://www.tyontech.com/"><img src="{{ asset('man.jpg') }}" width="100%"></a>
                        </div>
                        <div class="mb-m-clk">
                            <i class="fa fa-bars" aria-hidden="true"></i>
                        </div>
                        <div class="ui secondary menu desk-m-nav">
                            <a href="/home" class="active item">
                            Home
                        </a>
                            <a href="/apps" class="item app-store">
                            Apps
                        </a>
                            <div class="ui dropdown item" tabindex="0"> Pricing
                                <i class="caret down icon"></i>
                                <div class="menu margin-t16 transition hidden" tabindex="-1">
                                    <a class="item" href="/memberfactory/pricing">MemberFactory</a>
                                    <a class="item" href="/coachrack/pricing">CoachRack</a>
                                    <a class="item" href="/rewardburst/pricing">RewardBurst</a>
                                </div>
                            </div>
                            <a href="/academy" class="item academy-tab">
                            Academy
                            </a>
                            <a href="https://labs.invanto.com/" class="item">
                            Labs
                            </a>
                            <a href="https://community.invanto.com/" class="item">
                            Contact Us
                            </a>

                            <div class="right menu">
                                <a href="/signin" class="item">
                                Login
                            </a>
                                <a class="item" href="/signup">
                                    <button type="button" class="ui button green inverted mini">Sign Up</button>
                                </a>
                            </div>
                        </div>
                        <div class="mb-left-side-menu">
                            <div class="logo">
                                <a href="https://www.invanto.com/">
                                    <img src="//dffiles.s3.amazonaws.com/2/media/images/invanto.png" width="100%">
                                </a>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="overlay-right"></div>
                    </div>
                </div>
            </div>
        </div>
        <!--header Ends Here-->

        
            {{ form_start(form) }}
                {{ form_row(form.username) }}
                {{ form_row(form.password) }}
                
             <button type="submit">Register!</button>
            {{ form_end(form) }}


        <!--Footer-->
        <footer class="l-footer">
            <div class="ui container">
                <div class="ui grid margin0">
                    <div class="fourteen wide mobile eight wide computer column">
                        <h3>Get In Touch</h3>
                        <div class="socail-icon">
                            <a href="https://www.facebook.com/groups/tyontech/" target="_blank" style="color: #666666;"><i class="fa fa-facebook-square facebook" aria-hidden="true"></i></a>
                            <a href="https://twitter.com/tyontech" target="_blank" style="color: #666666;"><i class="fa fa-twitter-square twitter" aria-hidden="true"></i></a>
                            <a href="https://www.linkedin.com/company/tyontech" target="_blank" style="color: #666666;"><i class="fa fa-linkedin-square linkedin" aria-hidden="true"></i></a>
                        </div>
                        <p class="small">Or, email us at support@tyontech.com</p>
                    </div>
                    <div class="fourteen wide mobile eight wide computer column text-right">
                        <img src="/bundles/dotfogcore/om/images/logo-w.png">
                        <div class="ui secondary  menu">
                            <div class="right menu">
                                <a href="https://community.invanto.com/" class="item">
                            Help
                        </a>
                                <a href="https://labs.invanto.com/privacy/" class="item">
                            Privacy
                        </a>
                                <a href="https://labs.tyontech.com/terms/" class="item">
                            Terms
                        </a>
                            </div>
                        </div>
                        <p class="text small">Â© Copyright 2017 Tyontech Platform. All Rights Reserved.
                        </p>
                    </div>
                </div>
            </div>
        </footer>
    </header>



        {% endblock %}
        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
        <script src="assets/js/semantic.min.js"></script>
        {% endblock %}
    </body>
</html>
